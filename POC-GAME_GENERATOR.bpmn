<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   id="Definitions_1"
                   targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="GamePocGenerator" isExecutable="true">
    <bpmn:startEvent id="MyStart" name="Start Generation"/>
    <bpmn:sequenceFlow id="Flow1" sourceRef="MyStart" targetRef="TaskStoryLine" />
    <bpmn:scriptTask id="TaskStoryLine" name="Generate Storyline">
      <bpmn:script><![CDATA[
from agent_storyline import StorylineAgent
agent = StorylineAgent()
agent.generate()
      ]]></bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow2" sourceRef="TaskStoryLine" targetRef="MyEnd" />
    <bpmn:endEvent id="MyEnd" name="Storyline Generated"/>
  </bpmn:process>
</bpmn:definitions>